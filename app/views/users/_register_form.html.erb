<%= form_for register_path do |f| %>
  <%= f.label 'First Name' %>
  <%= f.text_field :first_name %>
  <br /><br />

  <%= f.label 'Last Name' %>
  <%= f.text_field :last_name %>
  <br /><br />

  <%= f.label 'Email' %>
  <%= f.text_field :email %>
  <br /><br />

  <%= f.label 'Username' %>
  <%= f.text_field :username %>
  <br /><br />

  <%= f.label 'Password' %>
  <%= f.text_field :password %>
  <br /><br />
  
  <%= submit_tag 'Create User' %>
<% end %>